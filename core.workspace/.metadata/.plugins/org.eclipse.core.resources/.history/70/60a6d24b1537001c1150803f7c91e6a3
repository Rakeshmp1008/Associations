package com.rakesh.hajama;

public class NaturalSaloon extends Saloon {

	public String name="NANU";
			public int noOfHajamas=3;
			
	public NaturalSaloon() {
		this("GURU");
		
	
		System.out.println("INVOKING NO ARG CONSTROCTOR");
	}
	
	public NaturalSaloon(String name) {
		this.name=name;
		System.out.println("INVKING STRING ARG CONSTROCTOR");
	}
}
