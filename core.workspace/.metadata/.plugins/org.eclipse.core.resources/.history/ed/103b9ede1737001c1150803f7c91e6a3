package com.rakesh.hajama;

public class Saloon {

	public String name="NINU";
	public int noOfHajamas=4;
	
	public Saloon() {
	
		System.out.println("INVOKING NO ARG CONSTRUCTOR---1");
	}
	
	public Saloon(String name) 
	{
		this.name=name;
		System.out.println("INVOKING STRING ARG CONSTRUCTOR----1");
	}
	
}
