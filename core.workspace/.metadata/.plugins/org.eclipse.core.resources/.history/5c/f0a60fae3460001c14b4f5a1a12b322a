package com.rakesh.functionalinterface.lambda.product;

public class ProductScammer {
	public static void main(String[] args) {

		ProductDAO dao=new ProductDAO();
		
		ProductDTO dtoCompare=new ProductDTO("TeaPOWDER","TAJmahal",500f);
		
		ProductDTO found=dao.find((p1,p2)->{
			if(p1.getPrice()==p2.getPrice() && p1.getProduct().equals(p2.getProduct())){
				return p1;
			}
			return null;
		},dtoCompare);
	}
}
