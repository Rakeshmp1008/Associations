package com.rakesh.dp.dto;

public class ClubDTO {

	private int noOfPeople;
	private int noOfBottels;
	private float rating;
	private float entryPrice;
	private int noOfStaff;
	
	public ClubDTO() {

	}

	public ClubDTO(int noOfPeople, int noOfBottels, float rating, float entryPrice, int noOfStaff) {
		super();
		this.noOfPeople = noOfPeople;
		this.noOfBottels = noOfBottels;
		this.rating = rating;
		this.entryPrice = entryPrice;
		this.noOfStaff = noOfStaff;
	}

	public int getNoOfPeople() {
		return noOfPeople;
	}

	public void setNoOfPeople(int noOfPeople) {
		this.noOfPeople = noOfPeople;
	}

	public int getNoOfBottels() {
		return noOfBottels;
	}

	public void setNoOfBottels(int noOfBottels) {
		this.noOfBottels = noOfBottels;
	}

	public float getRating() {
		return rating;
	}

	public void setRating(float rating) {
		this.rating = rating;
	}

	public float getEntryPrice() {
		return entryPrice;
	}

	public void setEntryPrice(float entryPrice) {
		this.entryPrice = entryPrice;
	}

	public int getNoOfStaff() {
		return noOfStaff;
	}

	public void setNoOfStaff(int noOfStaff) {
		this.noOfStaff = noOfStaff;
	}
	
}
