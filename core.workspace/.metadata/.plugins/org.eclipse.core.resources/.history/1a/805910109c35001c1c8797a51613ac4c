package com.rakesh.dp.dao;

import com.rakesh.dp.dto.WebSeriesDTO;

public class WebSeriesDAO {

	private WebSeriesDTO[] dto=new WebSeriesDTO[10];
	private int maga=0;
	
	public void saveWebSeries(WebSeriesDTO dto)
	{
		if(maga>this.dto.length) {
		this.dto[maga++]=dto;
		System.out.println("Invoking Create method".concat(String.valueOf(maga)));
		}
		else
		{
			System.out.println("ARRAY IS FULL");
		}
	}
}
