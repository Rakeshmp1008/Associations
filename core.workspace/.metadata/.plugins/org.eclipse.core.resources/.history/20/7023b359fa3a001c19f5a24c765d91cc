	public static void main(String[] args) {
		ApplicationDTO dtoref1=new ApplicationDTO("INSTAGRAM",182.3f,"FACEBOOK","RAKESH");
		ApplicationDTO dtoref2=new ApplicationDTO("PhonePay",10.3f,"Paycompany","Ayyappa");
